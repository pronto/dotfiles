execute pathogen#infect()

set ts=4
set t_Co=256
syntax on
colorscheme tango
set pastetoggle=<F12>
nmap <F8> :TagbarToggle<CR>
set shiftwidth=4
set cursorline
set autoindent
let g:pymode_syntax = 1
set expandtab
set copyindent
set ignorecase
set so=5
nnoremap j gj
nnoremap k gk
filetype indent on
set backspace=indent,eol,start
